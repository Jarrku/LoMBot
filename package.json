{
  "name": "lom_bot",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Simon VDB",
  "license": "MIT",
  "dependencies": {
    "bufferutil": "3.0.2",
    "discord.js": "11.2.1",
    "discord.js-commando": "0.9.0",
    "dotenv": "4.0.0",
    "pg": "7.3.0",
    "pg-hstore": "2.3.2",
    "sequelize": "4.13.17"
  },
  "devDependencies": {
    "@types/dotenv": "4.0.1",
    "@types/node": "8.0.30",
    "@types/sequelize": "4.0.76",
    "tslint": "5.7.0",
    "typescript": "2.5.2"
  },
  "scripts": {
    "transpile": "tsc",
    "transpile-w": "tsc --watch",
    "start": "node build/server.js",
    "deploy": "tsc && now --public && now alias"
  },
  "now": {
    "alias": "lom",
    "files": ["build/**/*.js"],
    "env": {
      "NODE_ENV": "production",
      "BOT_SECRET_PROD": "@lom_bot_secret_prod",
      "DB_USERNAME": "@lom_db_username",
      "DB_PASSWORD": "@lom_db_password",
      "DB_HOSTNAME": "@lom_db_hostname",
      "DB_NAME": "@lom_db_name"
    }
  }
}
